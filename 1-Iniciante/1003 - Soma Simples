		import java.io.IOException;
		import java.util.Scanner;
		public class Main {
		 
		    public static void main(String[] args) throws IOException {
		 
		         int a,b,SOMA;
			       Scanner in = new Scanner(System.in);
			       
			       a=in.nextInt();
			       b=in.nextInt();
			       
			       SOMA=a+b;
			       System.out.println("SOMA = " +SOMA);
		 
		    }
